<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.warehousemanager">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WarehouseManager"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activities.ChartActivity"
            android:exported="false" />
        <activity
            android:name=".activities.FeedBackActivity"
            android:exported="false" />
        <activity
            android:name=".activities.VatTuActivity_Edit"
            android:exported="true" />
        <activity
            android:name=".activities.VatTuActivity"
            android:exported="false" />
        <activity
            android:name=".activities.KhoActivity_Edit"
            android:exported="false" />
        <activity
            android:name=".activities.DetailPhieuNhapActivity_Edit"
            android:exported="false" />
        <activity
            android:name=".activities.KhoActivity"
            android:exported="false" />
        <activity
            android:name=".activities.DetailPhieuNhapActivity_Add"
            android:exported="false" />
        <activity
            android:name=".activities.PhieuNhapActivity_Add"
            android:exported="false" />
        <activity
            android:name=".activities.PhieuNhapActivity_Edit"
            android:exported="false" />
        <activity
            android:name=".activities.DetailPhieuNhapActivity"
            android:exported="false" />
        <activity
            android:name=".activities.PhieuNhapActivity"
            android:exported="false" />
        <activity
            android:name=".activities.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>